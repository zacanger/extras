{
  "compilerOptions": {
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "outDir": "./lib",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*"
  ]
}
